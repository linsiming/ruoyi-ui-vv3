{
  "name": "ruoyi-ui-vv3",
  "version": "0.0.0",
  "description": "RuoYi-Vue-Plus后台管理系统",
  "author": "wezone",
  "license": "MIT",
  "scripts": {
    "serve": "npm run dev",
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build ",
    "build:no-cache": "yarn clean:cache && npm run build",
    "build:test": "cross-env vite build --mode test ",
    "preview": "npm run build && vite preview",
    "preview:dist": "vite preview",
    "lint:js": "eslint \"{src,mock}/**/*.{vue,js}\" ",
    "lint:js-fix": "eslint \"{src,mock}/**/*.{vue,js}\" --fix",
    "lint:css": "stylelint \"**/*.{vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:css-fix": "stylelint \"**/*.{vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/ --fix",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run prettier",
      "npm run lint:js-fix",
      "npm run lint:css-fix",
      "git add ."
    ]
  },
  "keywords": [
    "vue3",
    "vite",
    "admin",
    "dashboard",
    "element-ui",
    "boilerplate",
    "admin-template",
    "management-system"
  ],
  "dependencies": {
    "@riophae/vue-treeselect": "^0.4.0",
    "axios": "^0.21.0",
    "clipboard": "^2.0.6",
    "core-js": "^3.6.5",
    "echarts": "^4.9.0",
    "element-plus": "^1.1.0-beta.9",
    "file-saver": "^2.0.4",
    "fuse.js": "^6.4.3",
    "highlight.js": "^9.18.5",
    "js-beautify": "^1.13.0",
    "js-cookie": "^2.2.1",
    "jsencrypt": "^3.0.0-rc.1",
    "nprogress": "^0.2.0",
    "screenfull": "^5.0.2",
    "sortablejs": "^1.10.2",
    "vue": "^3.2.6",
    "vue-cropper": "^0.5.6",
    "vue-meta": "^3.0.0-alpha.7",
    "vue-router": "^4.0.0-0",
    "vuedraggable": "2.24.3",
    "vuex": "^4.0.0-0"
  },
  "devDependencies": {
    "@babel/core": "7.4.5",
    "@babel/preset-env": "7.4.5",
    "@commitlint/cli": "12.0.1",
    "@commitlint/config-conventional": "12.0.1",
    "@vitejs/plugin-legacy": "^1.5.1",
    "@vitejs/plugin-vue": "^1.6.2",
    "@vitejs/plugin-vue-jsx": "^1.1.7",
    "@vue/compiler-sfc": "^3.2.11",
    "@vue/test-utils": "2.0.0-rc.6",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.6.3",
    "babel-plugin-transform-es2015-modules-commonjs": "6.26.2",
    "chalk": "^4.1.0",
    "connect": "^3.6.6",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "^10.0.0",
    "eslint": "^7.12.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-vue": "^7.17.0",
    "husky": "6.0.0",
    "jest": "25.0.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "quill": "^1.3.7",
    "rollup-plugin-visualizer": "^5.5.2",
    "sass": "^1.39.2",
    "sass-loader": "^12.1.0",
    "stylelint": "13.11.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "vite": "^2.5.6",
    "vite-plugin-compression": "^0.3.3",
    "vite-plugin-element-plus": "^0.0.12",
    "vite-plugin-html": "^2.0.7",
    "vite-plugin-purge-icons": "^0.7.0",
    "vite-plugin-pwa": "^0.10.0",
    "vite-plugin-style-import": "^1.1.1",
    "vite-plugin-svg-icons": "^1.0.4",
    "vite-plugin-theme": "^0.8.1",
    "vite-plugin-windicss": "^1.2.7"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
